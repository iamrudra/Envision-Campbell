<body>
	<div class="container-fluid">
   		<div class="container">
   			<div class="space_100px"></div>
   			<div class="space_30px"></div>
			<h1 class="bigTextNavMargin" style="margin-bottom:20px;">City <b>Connect</b></h1>
			<div class="col-md-2"></div>
   				<div class="col-md-8 card">
   				<div class="space_30px"></div>  
   				<div class="space_30px"></div>			
   				<form>
   				<div class="col-md-6">
  					<div class="form-group">
    					<label for="location">Location</label>
   						 <input type="number" class="form-control" id="location" placeholder="Zip Code">
  					</div>
				</div>
  				<div class="col-md-6">
  					<div class="form-group">
    					<label for="keywords">Key Words</label>
    					<input type="text" class="form-control" id="keywords" placeholder="Parking, Health...">
 			 		</div>
				</div>
  				<button type="submit" class="btn btn-default">Search</button>
				</form>
  				<div class="space_30px"></div>
  				<div class="space_30px"></div>
   				</div>
   			<div class="col-md-2"></div>

   		</div>
	</div>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
